# Copy this file to .env

# POSGTRES ENV
POSTGRES_DB=<docker_name>
POSTGRES_USER=<docker_user>
POSTGRES_PASSWORD=<docker_password>
DJANGO_POSTGRES_HOST=<docker_host>
DJANGO_POSTGRES_PORT=<docker_port>

# DJANGO ENV
DJANGO_DEBUG=0
DJANGO_ALLOWED_HOSTS=127.0.0.1,localhost,<project_name>.local
DJANGO_DATABASES_ENGINE=django.db.backends.sqlite3 #django.db.backends.postgresql_psycopg2/django.db.backends.mysql
